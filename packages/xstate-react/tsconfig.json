{
  "compilerOptions": {
    "outDir": "./lib",
    "allowJs": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es2017", "dom"],
    "moduleResolution": "node",
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "files": ["src/index.tsx"]
}
